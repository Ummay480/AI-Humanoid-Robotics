{
  "name": "@humanoid-robotics/web",
  "version": "1.0.0",
  "description": "Next.js web application for Humanoid Robotics Textbook",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
